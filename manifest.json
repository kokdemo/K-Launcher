{
  "name": "K-Launcher",
  "version": "0.9.3",
  "manifest_version": 2,
  "description": "一个提高你浏览效率的扩展",
  "options_page": "options.html",
  "icons": { 
      "48": "icon.png" 
  },
  "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*", 
        "notifications",
        "activeTab",
        "storage"
   ],
  "background": {
    "scripts": [ "/js/event.js" ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "K-Launcher"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Z",
        "mac": "Alt+Z"
      }
    }
  }
}